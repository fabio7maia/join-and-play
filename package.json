{
	"name": "join-and-play",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"postinstall": "prisma generate",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:errors": "eslint --quiet \"**/*.{ts,tsx}\"",
		"lint:fix": "eslint --fix\" **/*.{ts,tsx}\"",
		"prettier-format-all": "prettier --write \"**/*.{json,ts,tsx}\"",
		"lint:all": "npm run lint:errors && npm run spell:check",
		"spell:check": "cspell \"**/*.{ts,tsx}\"",
		"test": "jest \"--watchAll=false\" \"--forceExit\"",
		"test:coverage": "jest \"--coverage\" \"--watchAll=false\" \"--silent\" \"--forceExit\"",
		"prepare": "husky install",
		"git:pre-commit": "lint-staged",
		"git:pre-push": "npm run build && npm run test:coverage"
	},
